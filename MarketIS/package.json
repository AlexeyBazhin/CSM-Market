    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "babel-eslint": "10.1.0",
    "eslint": "^7.11.0",
    "eslint-plugin-react": "7.18.0"
